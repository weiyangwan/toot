<%= render 'users/left_nav' %>

<div class="container col-md-8">

  <div class="row">
    <%= render "posts/post_form" %>
  </div>

  <div class="row">
    <% if @user.posts.any? %>
      <ol class="userPosts">
        <%= render @posts %>
      </ol>
    <% end %>
    <%= will_paginate @posts %>
  </div>
</div>
